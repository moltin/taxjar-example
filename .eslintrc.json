{
  "extends": [
    "@moltin",
    "@moltin/eslint-config/rules/react",
    "@moltin/eslint-config/rules/react-a11y"
  ],
  "rules": {
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "hrefLeft",
          "hrefRight"
        ],
        "aspects": [
          "invalidHref",
          "preferButton"
        ]
      }
    ]
  }
}
